</html><script type="text/javascript" src="https://resource.eteams.cn/static/behavior/public/js/behavior.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            //进入登录
            $.userBehavior.send({
                'action_id':'login_001000100_0000099'
            });
            //判定js方法是否存在，确定是PC客户端还是其他
//             if(/electron/i.test(navigator.userAgent)){
//                 console.log('{ "webType" : "sessionExpired"}');
//             }
            if(window.IsEteamsClient && typeof(window.IsEteamsClient) == "function"){
                if(window.IsEteamsClient() == 1){
                    if(window.NeedKeepPsw()){
                        $('#j_rememberPwd').attr('checked',true);
                    }else{
                        $('#j_rememberPwd').attr('checked',false);
                    }
                    $('#j_pcClient').val("pc_client");
                } else if(window.IsEteamsClient() == 2){
                    if(window.NeedKeepPsw()){
                        $('#j_rememberPwd').attr('checked',true);
                    }else{
                        $('#j_rememberPwd').attr('checked',false);
                    }
                    $('#j_pcClient').val("mac_client");
                }
            }else{
                $('#j_rememberPwd').addClass("hide");
                $('.j_rememberPwd').addClass("hide");
            }
            $('#loginForm').find(':input').each(function(){
                var el = $(this);
                if(el.attr('id')&& !el.val()){
                    el.focus();
                    return false;
                }
            });
            $('#loginForm').submit(function(){
                var  filter = /^([A-Za-z0-9_\.-]+)@([\dA-Za-z\.-]+)\.([A-Za-z\.]{2,64})$/;
                var  username = $.trim($("#loginForm").find("#username").val());
                var  password = $.trim($("#loginForm").find("#password").val());
                if(username==""&&password==""){
                    $("#loginForm").find(".error").html("账号和密码均不能为空");
                    $("#loginForm").find("#username");
                    return false;
                }
                if(username==""){
                    $("#loginForm").find(".error").html("账号不能为空");
                    $("#loginForm").find("#username");
                    return false;
                }
                if(password==""){
                    $("#loginForm").find(".error").html("密码不能为空");
                    $("#loginForm").find("#password");
                    return false;
                }
                if(username.indexOf('@') >=0){
                    if(!filter.test(username)){
                        $("#loginForm").find(".error").html("邮箱格式不正确");
                        $("#loginForm").find("#username");
                        return false;
                    }
                }
                $(':submit').attr('disabled',true);
                return true;
            });
            $('.qrcode-num').hover(function(){
                    $('.wechat-qrcode').show('fast');
                },
                function(){
                    $('.wechat-qrcode').hide('fast');
                }).click(function(){
                return false;
            });
        });

        function rememberPwdChange(obj){
            if (obj.checked == false) {
                window.SetKeepPsw(false);
            } else {
                window.SetKeepPsw(true);
            }
        };
    </script>
